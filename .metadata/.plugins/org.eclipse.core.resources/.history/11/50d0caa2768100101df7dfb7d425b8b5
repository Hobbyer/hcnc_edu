package sample.web;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

import com.nexacro.uiadapter17.spring.core.annotation.ParamDataSet;
import com.nexacro.uiadapter17.spring.core.annotation.ParamVariable;
import com.nexacro.uiadapter17.spring.core.data.NexacroResult;

import sample.service.CodeService;

@Controller
public class CodeController {
  private Logger logger = LoggerFactory.getLogger(CodeController.class);
	
    @Resource(name = "codeService")
    private CodeService service;
    //부서 조회
    @RequestMapping(value = "/EduPjt_20240830_LAST/selectDeptCode.do")
    public NexacroResult selectDeptCode(@ParamDataSet(name = "ds_search", required = false) Map<String,Object> ds_param) {
    	NexacroResult result = new NexacroResult();
    	
    	try {
    		List<Map<String, Object>> selectDeptCode = service.selectDeptCode(ds_param);							//사용자 정보 조회
			
    		result.addDataSet("ds_dept", selectDeptCode);
    	}catch(Exception ee) {
    		System.out.println(ee);
    		result.setErrorCode(-1);
    		result.setErrorMsg("catch 조회 오류");
    	}
    	return result;
    }
    //하위부서 조회
    @RequestMapping(value = "/EduPjt_20240830_LAST/selectDeptSubCode.do")
    public NexacroResult selectDeptSubCode(@ParamDataSet(name = "ds_search", required = false) Map<String,Object> ds_param) {
    	NexacroResult result = new NexacroResult();
    	
    	try {
    		List<Map<String, Object>> selectDeptSubCode = service.selectDeptSubCode(ds_param);							//사용자 정보 조회
    		
    		result.addDataSet("ds_dept_sub", selectDeptSubCode);
    	}catch(Exception ee) {
    		System.out.println(ee);
    		result.setErrorCode(-1);
    		result.setErrorMsg("catch 조회 오류");
    	}
    	return result;
    }
    //직위 조회
    @RequestMapping(value = "/EduPjt_20240830_LAST/selectJikwiCode.do")
    public NexacroResult selectJikwiCode(@ParamDataSet(name = "ds_search", required = false) Map<String,Object> ds_param) {
    	NexacroResult result = new NexacroResult();
    	
    	try {
    		List<Map<String, Object>> selectJikwiCode = service.selectJikwiCode(ds_param);							//사용자 정보 조회
    		
    		result.addDataSet("ds_jikwi", selectJikwiCode);
    	}catch(Exception ee) {
    		System.out.println(ee);
    		result.setErrorCode(-1);
    		result.setErrorMsg("catch 조회 오류");
    	}
    	return result;
    }
    
    /**
     * 부서 코드 저장(등록/수정)
     * @param param
     * @return
     */
    @RequestMapping(value = "/EduPjt_20240830_LAST/saveCodeDept.do")
    public NexacroResult saveCodeDept(@ParamDataSet(name = "ds_deptIst", required = false) Map<String,Object> param){
    	NexacroResult result = new NexacroResult();
    	try {
    		service.saveCodeDept(param);
    	}catch(Exception ee) {
    		System.out.println(ee);
    		result.setErrorCode(-1);
    		result.setErrorMsg("catch 조회 오류");
    	}
    	return result;
    }

    /**
     * 부서 코드 삭제
     * @param param
     * @return
     */
    @RequestMapping(value = "/EduPjt_20240830_LAST/deleteCodeDept.do")
