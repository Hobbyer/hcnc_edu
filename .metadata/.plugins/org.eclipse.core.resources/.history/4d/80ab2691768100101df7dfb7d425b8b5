package sample.service;

import java.util.List;
import java.util.Map;

public interface LoginService {
	/**
	 * 사용자 유무
	 * @param param
	 */
	int selectUserCnt(Map<String, Object> ds_param);

	/**
	 * 사용자 조회
	 * @param param
	 * @return
	 */
	Map<String, Object> selectUser(Map<String, Object> ds_param);

	/**
	 * 사용자 추가
	 * @param param
	 */
	void insertUser(Map<String, Object> param);
	
	/**
	 * 전체 회원 조회
	 * @param param
	 * @return
	 */
	List<Map<String, Object>> selectUsers(Map<String, Object> ds_param);
	
	/**
	 * 마스터 코드 저장(등록/수정)
	 * @param param
	 */
	void saveCodeMst(Map<String, Object> param);

	/**
	 * 마스터 코드 삭제
	 * @param param
	 */
	void deleteCodeMst(Map<String, Object> param);

	/**
	 * 세부 코드 리스트 조회
	 * @param param
	 * @return
	 */
	List<Map<String, Object>> selectCodeDtl(Map<String, Object> param);

	/**
	 * 세부 코드 저장(등록/수정)
	 * @param param
	 */
	void saveCodeDtl(Map<String, Object> param);

	/**
	 * 세부 코드 삭제
	 * @param param
	 */
	void deleteCodeDtl(Map<String, Object> param);

}